<% include ../partials/header.ejs%>
	<% include ../partials/auth_nav.ejs%>


		<div class="page-header">
			<h1 class="center-block"><small>Welcome back <span id="username" data-username="username"> <%=welcomeName%>! </span></small></h1>
		</div>
		<div class="row">
			<div class="schoolContainer bg-info droppable">

				<div class="col-sm-offset-1 col-sm-5 pull-left">
					<h2>My Unsorted Schools</h2>
					<ul class="sortable list schoolList" id="unsortedList">
						<% if (greyList) {greyList.forEach(function(school){ %>
							<li class="draggable sortable">
								<%=school%>
							</li>
							<% }) } %>


					</ul>
				</div>
			</div>

			<div class="schoolContainer bg-info droppable">
				<div class="col-sm-offset-1 col-sm-5 pull-right">
					<h2>My Maybe Schools</h2>
					<ul class="sortable list schoolList" id="maybeList">
						<% if (yellowList) {yellowList.forEach(function(school){ %>
							<li class="draggable sortable">
								<%=school%>
							</li>
							<% })} %>
					</ul>
				</div>
			</div>
		</div>

		<div class="row">
			<div class="schoolContainer bg-info droppable">
				<div class="col-sm-offset-1 col-sm-5 pull-left">
					<h2>My Probably Schools</h2>
					<ul class="sortable list schoolList" id="probablyList">
						<% if (orangeList) {orangeList.forEach(function(school){ %>
							<li class="draggable sortable">
								<%=school%>
							</li>
							<% }) }%>

					</ul>
				</div>
			</div>
			<div class="schoolContainer bg-info droppable">
				<div class="col-sm-offset-1 col-sm-5 pull-right">
					<h2>My Definitely Schools</h2>
					<ul class="sortable list schoolList" id="maybeList">
						<% if (greenList) {greenList.forEach(function(school){ %>
							<li class="draggable sortable">
								<%=school%>
							</li>
							<% }) }%>

					</ul>
				</div>
			</div>
		</div>

		<% include ../partials/footer.ejs%>