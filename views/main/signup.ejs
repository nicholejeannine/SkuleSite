		<div class="row">
			<div class="col-xs-offset-3 col-xs-6 col-md-offset-4 col-md-4 col-lg-offset-5 col-lg-2">
				<form method="post" action="/user/signup">
					<div class="form-group">
						<label for="username">Choose A User Name:</label>
						<input name="username" type="text" class="form-control" id="username" placeholder="Enter a user name:">
					</div>
					<div class="form-group">
						<label for="password">Password</label>
						<input name="password" type="password" class="form-control" id="password" placeholder="Password">
					</div>
					<div class="form-group">
						<label for="password">Confirm Your Password:</label>
						<input type="password" class="form-control" id="passwordCheck" placeholder="Password">
					</div>

					<button id="signupSubmit" type="submit" class="btn btn-default">Submit</button>
				</form>
			</div>
		</div>
